{
  "private": true,
  "dependencies": {
    "async": "^0.9.0",
    "babel": "^4.7.16",
    "babelify": "^5.0.4",
    "browserify": "^9.0.3",
    "connect-cachify": "0.0.17",
    "ejs": "^2.3.1",
    "express": "^4.12.3",
    "isomorphic-fetch": "^2.0.0",
    "react": "^0.13.1",
    "react-router": "^0.13.2",
    "request": "^2.54.0",
    "timeago": "^0.2.0",
    "uglifyjs": "^2.4.10"
  },
  "devDependencies": {
    "nodemon": "^1.3.7",
    "watchify": "^2.4.0"
  },
  "scripts": {
    "start": "node server.js",
    "live": "npm run js:watch & nodemon server.js",
    "postinstall": "npm run js:build && npm run js:minify",
    "js:build": "browserify src/app.jsx -t babelify --outfile assets/app.js",
    "js:minify": "uglifyjs assets/app.js -o assets/app.min.js",
    "js:watch": "watchify src/app.jsx -t babelify --outfile assets/app.js",
    "download:all": "curl -L http://registry.npmjs.eu/-/all -o data/npm.json",
    "download:recent": "npm run download:today && npm run download:yesterday",
    "download:today": "curl -L https://registry.npmjs.org/-/all/static/today.json -o data/npm.json",
    "download:yesterday": "curl -L https://registry.npmjs.org/-/all/static/yesterday.json -o data/npm.json",
    "fetch": "node fetch.js",
    "parse": "node parse.js"
  }
}
